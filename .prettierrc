{
	"semi": true,
	"trailingComma": "es5",
	"singleQuote": true,
	"jsxSingleQuote": true,
	"printWidth": 120,
	"tabWidth": 2,
	"useTabs": true,
	"bracketSpacing": true,
	"arrowParens": "always",
	"proseWrap": "never",
	"htmlWhitespaceSensitivity": "strict",
	"endOfLine": "lf",
	"plugins": ["prettier-plugin-tailwindcss"]
}
